USER LIST
<table class="content_tbl">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr class="<%= cycle("", "even") %>">
    <td><%= link_to user.full_name, user %></td>
    <td><%= user.email %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %> | <%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>

